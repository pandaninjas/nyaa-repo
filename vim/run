#!/bin/sh
NAME="vim"
RELEASE="1"
VERSION="9.0.1188"
SOURCES="https://github.com/vim/vim/archive/refs/tags/v$VERSION.tar.gz"
SHA256SUM="ff7f5a5dd5b657edc48e544b39736ff50cb1994bbec2dfe8e13cca94d8ad752e  v9.0.1188.tar.gz"
DESCRIPTION="Vi Improved"

build() {
    make -j$(nproc)
}

install() {
    make install
}
