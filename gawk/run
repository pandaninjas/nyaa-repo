#!/bin/sh
NAME="gawk"
RELEASE="1"
VERSION="5.2.1"
SOURCES="https://ftp.gnu.org/pub/gnu/gawk/gawk-$VERSION.tar.xz"
SHA256SUM="673553b91f9e18cc5792ed51075df8d510c9040f550a6f74e09c9add243a7e4f  gawk-5.2.1.tar.xz"
DESCRIPTION="GNU awk"

build() {
    ./configure --prefix=/usr
}

install() {
    make DESTDIR=$ROOT install
}
