#!/bin/sh
NAME="gmp"
RELEASE="1"
VERSION="6.2.1"
SOURCES="https://ftp.gnu.org/gnu/gmp/gmp-$VERSION.tar.xz"
SHA256SUM="fd4829912cddd12f84181c3451cc752be224643e87fac497b69edddadc49b4f2  gmp-6.2.1.tar.xz"
DESCRIPTION="GNU gmp"

build() {
    cd gmp-$VERSION
    ./configure --prefix=/usr
    make -j$(nproc)
}

install() {
    make DESTDIR=$ROOT install
}

